{
  "name": "cesar-monorepo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -r node_modules bun.lock",
    "generate": "turbo run generate",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.11",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "apollo-upload-client": "^18.0.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-63e3235-20250105",
    "graphql-tag": "^2.12.6",
    "object-path": "^0.11.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recursive-iterator": "^3.3.0"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.43",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "patchedDependencies": {
    "@faustwp/experimental-app-router@0.4.0": "patches/@faustwp%2Fexperimental-app-router@0.4.0.patch"
  }
}